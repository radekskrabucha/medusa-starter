{
  "name": "@medusa-starter/medusa",
  "version": "1.0.0",
  "private": true,
  "description": "A starter for Medusa projects.",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "medusa develop",
    "build": "medusa build",
    "start": "medusa start",
    "format": "prettier ./src --write",
    "lint": "eslint \"src/**/*.+(ts|js|tsx|jsx)\"",
    "lint:fix": "eslint \"src/**/*.+(ts|js|tsx|jsx)\" --fix",
    "type-check": "tsc --noEmit",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.5.1",
    "@medusajs/cli": "2.5.1",
    "@medusajs/framework": "2.5.1",
    "@medusajs/medusa": "2.5.1",
    "@mikro-orm/core": "6.4.7",
    "@mikro-orm/knex": "6.4.7",
    "@mikro-orm/migrations": "6.4.7",
    "@mikro-orm/postgresql": "6.4.7",
    "awilix": "12.0.4",
    "pg": "8.13.3"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.5.1",
    "@mikro-orm/cli": "6.4.7",
    "@swc/core": "1.11.1",
    "@swc/jest": "0.2.37",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.5",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "jest": "29.7.0",
    "prop-types": "15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "vite": "6.2.0",
    "yalc": "1.0.0-pre.53"
  }
}